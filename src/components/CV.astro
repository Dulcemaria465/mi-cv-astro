<style>
  * { margin: 0; padding: 0; box-sizing: border-box; }
  body { font-family: 'Inter', sans-serif; background-color: #f5f1eb; color: #4a4a4a; line-height: 1.6; }

  .resume-container {
    max-width: 1200px; margin: 0 auto; padding: 40px 20px;
    display: grid; grid-template-columns: 1fr 1.5fr;
    gap: 60px; min-height: 100vh;
  }

  .left-column, .right-column { display: flex; flex-direction: column; gap: 40px; }

  .profile-section { text-align: center; }
  .profile-image { width: 200px; height: 200px; border-radius: 50%; object-fit: cover; margin: 0 auto 20px; }
  .name { font-size: 2.5rem; font-weight: 700; color: #6b7280; margin-bottom: 5px; }
  .title { font-size: 1.5rem; font-weight: 300; color: #9ca3af; font-style: italic; }
  .personal-info { display: flex; flex-direction: column; gap: 8px; margin-top: 20px; }
  .info-item { display: flex; align-items: center; gap: 10px; font-size: 0.9rem; }
  .info-label { font-weight: 600; color: #6b7280; min-width: 80px; }

  .section-title { font-size: 1.8rem; font-weight: 700; color: #6b7280; margin-bottom: 20px; border-bottom: 2px solid #d1d5db; padding-bottom: 10px; }
  .education-item, .contact-item, .cert-item { margin-bottom: 20px; }
  .education-school { color: #9ca3af; font-size: 0.9rem; }

  .contact-item { display: flex; align-items: center; gap: 15px; padding: 10px; background: rgba(255,255,255,0.3); border-radius: 8px; }
  .contact-icon { width: 24px; height: 24px; background: #6b7280; border-radius: 4px; display: flex; align-items: center; justify-content: center; color: white; font-size: 12px; }

  .cert-section { display: flex; flex-direction: column; gap: 15px; }
  .cert-item { padding: 10px; border: 1px solid #d1d5db; border-radius: 8px; background: #fff; }
  .cert-item img { width: 120px; margin-top: 10px; }

  .download-section { text-align: center; margin-top: 20px; }
  .download-btn { display: inline-block; background: #6b7280; color: white; font-weight: 600; text-decoration: none; padding: 12px 20px; border-radius: 8px; transition: background 0.3s ease; }
  .download-btn:hover { background: #4b5563; }

  @media (max-width: 768px) {
    .resume-container { grid-template-columns: 1fr; gap: 30px; }
    .name { font-size: 2rem; }
    .title { font-size: 1.2rem; }
  }
</style>
